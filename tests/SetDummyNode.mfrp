module SetDummyNode
in
  x : Int
out
  y : Int
use
  Std

node init[0] counter = counter@last + 1
node happy100 = if counter == 100 then 100 else 0
node y = x + happy100

node const99 = 99
node init[100] dummyCounter = const99 + 1

#@ :set-dummy-node counter => dummyCounter
#@ :assert-module
#-   5 => 105
#@ : 1 => 101
#@ : 2 => 102
